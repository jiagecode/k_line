<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.line.backstage.dao.mapper.SkuInfoMapper">

    <resultMap type="com.line.backstage.entity.SkuInfo" id="SkuInfoMap">
        <result property="skuId" column="SKU_ID" jdbcType="INTEGER"/>
        <result property="skuCode" column="SKU_CODE" jdbcType="VARCHAR"/>
        <result property="skuName" column="SKU_NAME" jdbcType="VARCHAR"/>
        <result property="skuStatus" column="SKU_STATUS" jdbcType="INTEGER"/>
        <result property="nowPrice" column="NOW_PRICE" jdbcType="NUMERIC"/>
        <result property="minPrice" column="MIN_PRICE" jdbcType="NUMERIC"/>
        <result property="maxPrice" column="MAX_PRICE" jdbcType="NUMERIC"/>
        <result property="openPrice" column="OPEN_PRICE" jdbcType="NUMERIC"/>
        <result property="addDate" column="ADD_DATE" jdbcType="TIMESTAMP"/>
        <result property="editDate" column="EDIT_DATE" jdbcType="TIMESTAMP"/>
        <result property="skuType" column="SKU_TYPE" jdbcType="INTEGER"/>
        <result property="skuCycle" column="SKU_CYCLE" jdbcType="INTEGER"/>
        <result property="skuIncomeRate" column="SKU_INCOME_RATE" jdbcType="NUMERIC"/>
        <result property="addUserId" column="ADD_USER_ID" jdbcType="INTEGER"/>
        <result property="editUserId" column="EDIT_USER_ID" jdbcType="INTEGER"/>
        <result property="del" column="DEL" jdbcType="INTEGER"/>
    </resultMap>
    <select id="queryListAll" resultType="com.line.backstage.vo.SkuInfoVo">
        select SKU_ID,SKU_CODE,SKU_NAME,NOW_PRICE,MAX_PRICE,MIN_PRICE,OPEN_PRICE from t_sku_info where SKU_STATUS=1 AND DEL=1
    </select>
    <select id="updetaBySkuInfoVo" parameterType="com.line.backstage.vo.SkuInfoVo" >
        UPDATE t_sku_info SET NOW_PRICE=#{nowPrice},MIN_PRICE=#{minPrice},MAX_PRICE=#{maxPrice},OPEN_PRICE=#{openPrice} WHERE SKU_CODE=#{skuCode}
    </select>
</mapper>